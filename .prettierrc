{
  "singleQuote": true,
  "tabWidth": 2,
  "semi": true,
  "arrowParens": "always",
  "printWidth": 300,
  "bracketSpacing": true,
  "insertPragma": false,
  "jsxBracketSameLine": true,
  "proseWrap": "never",
